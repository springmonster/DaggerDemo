<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="demo.dagger.com.daggerdemo">

    <application
            android:name=".global.MyApp"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".inject.InjectActivity">
        </activity>
        <activity android:name=".component.ComponentActivity">
        </activity>
        <activity android:name=".qualtifier.QualifierActivity">
        </activity>
        <activity android:name=".siblingscomponent.SiblingsActivity">
        </activity>
        <activity android:name=".extendcomponentone.ExtendOneComponentActivity">
        </activity>
        <activity android:name=".extendcomponenttwo.ExtendTwoComponentActivity">
        </activity>
        <activity android:name=".scopeone.ScopeOneActivity">
        </activity>
        <activity android:name=".scopetwo.ScopeTwoActivity">
        </activity>
        <activity android:name=".scopetwo.ScopeTwoAnotherActivity">
        </activity>
        <activity android:name=".bindinstance.BindInstanceActivity">
        </activity>
        <activity android:name=".lazy.LazyActivity">
        </activity>
        <activity android:name=".global.GlobalTestActivity">
        </activity>
        <activity android:name=".binds.BindsActivity">
        </activity>
        <activity android:name=".intomapintoset.IntoMapIntoSetActivity">
        </activity>
    </application>

</manifest>